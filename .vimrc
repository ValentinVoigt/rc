set swapfile
set dir=~/.vim/tmp
set backupdir=~/.vim/backups
set cindent
set autochdir

syntax on
"filetype plugin indent on
set showcmd
set showmatch
set ignorecase
set mouse=a

set pastetoggle=<F5>

" Spaces
"set expandtab
"set shiftwidth=4
"set softtabstop=4

" Tabs
set noexpandtab
set tabstop=4
set shiftwidth=4
